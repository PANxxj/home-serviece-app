{
  "expo": {
    "name": "home-serviece-app",
    "slug": "home-serviece-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/application.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/application.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/application.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.homeservieceapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
